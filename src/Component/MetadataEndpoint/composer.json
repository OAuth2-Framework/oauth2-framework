{
    "name": "oauth2-framework/metadata-endpoint",
    "type": "library",
    "description": "Metadata Endpoint for the OAuth2 Framework",
    "license": "MIT",
    "keywords": ["RFC6749", "oauth2", "framework", "metadata", "endpoint", "library"],
    "homepage": "https://oauth2-framework.spomky-labs.com/",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/OAuth2-Framework/oauth2-framework/contributors"
        }
    ],
    "require": {
        "php": "^7.3",
        "oauth2-framework/core": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "php-http/message": "^1.0"
    },
    "suggest": {
        "web-token/jwt-signature": "For signature of the metadata."
    },
    "autoload": {
        "psr-4": {
            "OAuth2Framework\\Component\\MetadataEndpoint\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
