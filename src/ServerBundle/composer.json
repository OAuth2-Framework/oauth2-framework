{
    "name": "oauth2-framework/server-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 OAuth2 Server Bundle",
    "keywords": ["OAuth2", "Server", "Bundle", "Symfony2"],
    "homepage": "https://github.com/OAuth2-Framework/server-bundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.2",
        "sensio/framework-extra-bundle": "^3.0.8",
        "spomky-labs/jose-bundle": "^2.2",
        "simple-bus/symfony-bridge": "^4.1",
        "mnapoli/fluent-symfony": "^0.1.5",
        "oauth2-framework/server-library": "^1.0@dev",
        "league/json-guard": "dev-master"
    },
    "require-dev": {
        "behat/symfony2-extension": "^2.1",
        "behat/behat": "^3.1",
        "behat/mink": "^1.5",
        "behat/mink-extension": "^2.0",
        "behat/mink-goutte-driver":  "^1.2",
        "symfony/dom-crawler": "^3.2",
        "symfony/var-dumper": "^3.2",
        "doctrine/annotations": "^1.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.3",
        "http-interop/http-factory-diactoros": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "OAuth2Framework\\ServerBundle\\Server\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OAuth2Framework\\ServerBundle\\Server\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
